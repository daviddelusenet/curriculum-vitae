{
    "name": "curriculum-vitae",
    "version": "1.1.1",
    "description": "Curriculum vitae of David de Lusenet",
    "main": "src/index.js",
    "scripts": {
        "start": "npm run build:dev",
        "build:dev": "BABEL_ENV=dev webpack-dev-server --hot --config config/webpack.dev.js --mode development",
        "build:prod": "BABEL_ENV=prod webpack --config config/webpack.prod.js --mode production",
        "check-types": "tsc",
        "dist": "npm run build:prod",
        "dist:analyze": "BABEL_ENV=prod webpack --config config/webpack.prod.js --mode production --env.analyze=true",
        "lint:all": "npm run lint:ts && npm run lint:css && npm run check-types",
        "lint:css": "stylelint './src/**/*'",
        "lint:ts": "eslint './src/**/*'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/daviddelusenet/curriculum-vitae.git"
    },
    "keywords": [
        "Curriculum",
        "vitae",
        "React",
        "Styled",
        "Components"
    ],
    "author": "David de Lusenet",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/daviddelusenet/curriculum-vitae/issues"
    },
    "homepage": "https://github.com/daviddelusenet/curriculum-vitae#readme",
    "dependencies": {
        "@hot-loader/react-dom": "^16.13.0",
        "core-js": "^3.6.5",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-hot-loader": "^4.12.21",
        "react-svg": "^11.0.31",
        "styled-components": "^5.1.1",
        "styled-media-query": "^2.1.2",
        "styled-reset-advanced": "^1.0.3"
    },
    "devDependencies": {
        "@babel/core": "^7.10.5",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-react": "^7.10.4",
        "@babel/preset-typescript": "^7.10.4",
        "@types/react-dom": "^16.9.8",
        "@types/styled-components": "^5.1.1",
        "@typescript-eslint/eslint-plugin": "^3.6.1",
        "@typescript-eslint/parser": "^3.6.1",
        "babel-loader": "^8.1.0",
        "babel-plugin-styled-components": "^1.10.7",
        "clean-webpack-plugin": "^3.0.0",
        "compression-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^6.0.3",
        "eslint": "^7.5.0",
        "eslint-config-airbnb-typescript": "^9.0.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.3",
        "eslint-plugin-typescript-sort-keys": "^1.3.0",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^4.3.0",
        "prettier": "^2.0.5",
        "stylelint": "^13.6.1",
        "stylelint-config-concentric-order": "^4.0.0",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-prettier": "^1.1.2",
        "typescript": "^3.9.7",
        "webpack": "^4.43.0",
        "webpack-bundle-analyzer": "^3.8.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "webpack-jarvis": "^0.3.2",
        "webpack-merge": "^5.0.9"
    }
}
