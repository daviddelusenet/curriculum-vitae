{
    "name": "curriculum-vitae",
    "version": "1.1.1",
    "description": "Curriculum vitae of David de Lusenet",
    "main": "src/index.js",
    "scripts": {
        "start": "npm run build:dev",
        "build:dev": "BABEL_ENV=dev webpack-dev-server --hot --config config/webpack.dev.js --mode development",
        "build:prod": "BABEL_ENV=prod webpack --config config/webpack.prod.js --mode production",
        "dist": "npm run build:prod",
        "dist:analyze": "BABEL_ENV=prod webpack --config config/webpack.prod.js --mode production --env.analyze=true",
        "lint:all": "npm run lint:js && npm run lint:css",
        "lint:css": "stylelint './src/**/*.js' --config config/.stylelintrc.js",
        "lint:js": "eslint './src/**/*.js'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/daviddelusenet/curriculum-vitae.git"
    },
    "keywords": [
        "Curriculum",
        "vitae",
        "React",
        "Styled",
        "Components"
    ],
    "author": "David de Lusenet",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/daviddelusenet/curriculum-vitae/issues"
    },
    "homepage": "https://github.com/daviddelusenet/curriculum-vitae#readme",
    "dependencies": {
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-hot-loader": "^4.12.21",
        "react-svg": "^11.0.25",
        "styled-components": "^5.1.1",
        "styled-media-query": "^2.1.2",
        "styled-reset-advanced": "^1.0.3"
    },
    "devDependencies": {
        "@babel/core": "^7.10.2",
        "@babel/preset-env": "^7.10.2",
        "@babel/preset-react": "^7.10.1",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-styled-components": "^1.10.7",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "clean-webpack-plugin": "^3.0.0",
        "compression-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^6.0.2",
        "eslint": "^7.2.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.20.0",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^4.3.0",
        "prettier": "^2.0.5",
        "stylelint": "^13.6.0",
        "stylelint-config-concentric-order": "^4.0.0",
        "stylelint-config-prettier": "^8.0.1",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-prettier": "^1.1.2",
        "webpack": "^4.43.0",
        "webpack-bundle-analyzer": "^3.8.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0",
        "webpack-jarvis": "^0.3.2",
        "webpack-merge": "^4.2.2"
    }
}
